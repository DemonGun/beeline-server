{
    "env": {
        "es6": true,
        "node": true
    },
    "parser": "babel-eslint",
    "plugins": [
        "babel"
    ],
    "extends": [
        "eslint:recommended",
        "google",
        "prettier"
    ],
    "rules": {
        "no-unused-vars": ["error"],
        "semi": ["error", "never"],
        "comma-dangle": ["error", "always-multiline"],
        "radix": ["error", "as-needed"],
        "require-jsdoc": ["warn"],
        "no-console": ["error", {"allow": ["warn", "error"]}],
        "one-var": ["off"]
    }
}
